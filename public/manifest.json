{
    "name": "SigmaWallet",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
      "default_title": "SigmaWallet",
      "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}